{
  "publisher": "kittybbit",
  "name": "vscode-ajsbutler",
  "displayName": "vscode-ajsbutler",
  "description": "Support tool for JP1/AJS3",
  "version": "1.11.3",
  "private": true,
  "license": "MIT",
  "homepage": "https://github.com/kittybbit/vscode-ajsbutler",
  "repository": {
    "type": "git",
    "url": "https://github.com/kittybbit/vscode-ajsbutler"
  },
  "bugs": {
    "url": "https://github.com/kittybbit/vscode-ajsbutler/issues"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "JP1/AJS"
  ],
  "main": "./out/extension.js",
  "browser": "./out/web.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "ajsbutler.tableViewer",
        "displayName": "JP1/AJS table viewer",
        "selector": [
          {
            "filenamePattern": "*"
          }
        ],
        "priority": "option"
      },
      {
        "viewType": "ajsbutler.flowViewer",
        "displayName": "JP1/AJS flow viewer",
        "selector": [
          {
            "filenamePattern": "*"
          }
        ],
        "priority": "option"
      }
    ],
    "commands": [
      {
        "enablement": "editorLangId == 'jp1ajs'",
        "command": "open.ajsbutler.flowViewer",
        "category": "View",
        "title": "Open JP1/AJS flow viewer",
        "shortTitle": "JP1/AJS flow viewer",
        "icon": "$(circuit-board)"
      },
      {
        "enablement": "editorLangId == 'jp1ajs'",
        "command": "open.ajsbutler.tableViewer",
        "category": "View",
        "title": "Open JP1/AJS table viewer",
        "shortTitle": "JP1/AJS table viewer",
        "icon": "$(output)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "editorLangId == 'jp1ajs'",
          "command": "open.ajsbutler.flowViewer",
          "group": "navigation"
        },
        {
          "when": "editorLangId == 'jp1ajs'",
          "command": "open.ajsbutler.tableViewer",
          "group": "navigation"
        }
      ]
    },
    "languages": [
      {
        "id": "jp1ajs",
        "aliases": [
          "JP1/AJS",
          "jp1ajs",
          "JP1",
          "AJS"
        ],
        "extensions": [
          ""
        ],
        "configuration": "./language/language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "jp1ajs",
        "scopeName": "source.ajs",
        "path": "./syntax/jp1ajs.tmLanguage.json"
      }
    ]
  },
  "activationEvents": [
    "onWebviewPanel:ajsbutler.tableViewer",
    "onWebviewPanel:ajsbutler.flowViewer"
  ],
  "scripts": {
    "vscode:prepublish": "npm run build",
    "clean": "rimraf out report",
    "predevelopment": "npm-run-all qlty",
    "development": "webpack",
    "prebuild": "npm-run-all clean antlr4ts qlty",
    "build": "webpack --mode production",
    "antlr4ts": "npm-run-all antlr:clean antlr:generate",
    "antlr:clean": "rimraf src/generate/parser",
    "antlr:generate": "antlr4ts -o src/generate/parser -Xexact-output-dir src/antlr/*.g4",
    "watch": "webpack -w",
    "open-in-browser": "vscode-test-web --extensionDevelopmentPath=. .",
    "pretest": "npm-run-all development",
    "qlty": "npm-run-all qlty:fmt qlty:check",
    "qlty:fmt": "qlty fmt",
    "qlty:check": "qlty check",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@types/glob": "^9.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.0.10",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.8",
    "@types/styled-components": "^5.1.36",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.75.0",
    "@types/vscode-webview": "^1.57.5",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/test-web": "^0.0.76",
    "@vscode/vsce": "^3.7.0",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "css-loader": "^7.1.2",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "glob": "^11.1.0",
    "globals": "^16.5.0",
    "mocha": "^11.7.5",
    "npm-run-all": "^4.1.5",
    "process": "^0.11.10",
    "rimraf": "^6.1.2",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "webpack": "^5.103.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@tanstack/match-sorter-utils": "^8.19.4",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/table-core": "^8.21.3",
    "@vscode/extension-telemetry": "^1.2.0",
    "@xyflow/react": "^12.9.3",
    "antlr4ts": "^0.5.0-alpha.4",
    "assert": "^2.1.0",
    "classnames": "^2.5.1",
    "flatted": "^3.3.3",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-virtuoso": "^4.14.1",
    "util": "^0.12.5",
    "uuid": "^11.1.0",
    "xxhash-wasm": "^1.1.0"
  },
  "overrides": {
    "elliptic": "^6.5.6",
    "ws": ">=7.5.10"
  }
}
