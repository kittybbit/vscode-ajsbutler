{
    "comments": {
        "blockComment": [ "/*", "*/" ]
    },
    "brackets": [
        ["{", "}"],
        ["(", ")"]
    ],
    "autoClosingPairs": [
        ["{", "}"],
        ["(", ")"],
        ["\"", "\""]
    ],
    "surroundingPairs": [
        ["{", "}"],
        ["(", ")"],
        ["\"", "\""]
    ],
    "indentationRules": {
        "increaseIndentPattern": "{",
        "decreaseIndentPattern": "}"
    },
    "folding": {
        "markers": {
            "start": "{",
            "end": "}"
        }
    }
}